!/usr/bin/env bash
# Bash script that ddisplays numbers from 1 to 20 & does stuff.

count_=1

while [ $count_ -le 20 ]
do
  case $count_ in
    "4") echo "$count_ bad luck from China";;
    "9") echo "$count_ bad luck from Japan";;
    "17") echo "$count_ bad luck from Italy";;
    *) echo $count_;;
  esac
  ((count_++))
done
